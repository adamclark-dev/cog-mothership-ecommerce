{% extends 'Message:Mothership:ControlPanel::_templates/main' %}

{% block javascripts %}
	{{ parent() }}
	{% javascripts
		'@Message:Mothership:Ecommerce::resources/assets/js/jZebraControl.jquery.js'
		'@Message:Mothership:Ecommerce::resources/assets/js/fulfillment.js'

		output='/assets/js/ms_cp_order_fulfillment.js'
	%}
		<script src="{{ asset_url }}"></script>
	{% endjavascripts %}
	<script>
		$.jZebraControl.configure({
			printerName: {{ app.cfg.dispatch.printerName|json_encode|raw }}
		});

		$(function() {
			$.jZebraControl.append({{ contents|json_encode|raw }});
			$.jZebraControl.print();
		});
	</script>
{% endblock %}

{% block body %}

{% endblock %}